{
  "expo": {
    "name": "Innerbloom Mobile",
    "slug": "innerbloom-mobile",
    "scheme": "innerbloom",
    "version": "1.0.0",
    "orientation": "portrait",
    "sdkVersion": "54.0.0",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.innerbloom.mobile"
    },
    "android": {
      "package": "com.innerbloom.mobile"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-secure-store",
      "expo-dev-client"
    ]
  }
}
